{
  "name": "editorconfig-jxa",
  "version": "0.0.0-development",
  "description": "EditorConfig core library for JXA (JavaScript for Automation) on macOS",
  "keywords": [
    "jxa",
    "osx",
    "macos"
  ],
  "homepage": "https://github.com/umireon/editorconfig-core-jxa",
  "bugs": {
    "url": "https://github.com/umireon/editorconfig-core-jxa/issues"
  },
  "license": "MIT",
  "author": "Kaito Udagawa <umireon@gmail.com>",
  "files": [
    "dist/*"
  ],
  "main": "dist/editorconfig.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/umireon/editorconfig-jxa.git"
  },
  "scripts": {
    "build": "npm run cjs && npm run scpt",
    "cjs": "rollup -c -f cjs -o dist/editorconfig.js",
    "scpt": "rollup -c -f cjs --intro '!function(exports){' --outro '}(this)' | osacompile -l JavaScript -o dist/editorconfig.scpt",
    "semantic-release": "semantic-release pre && npm run cjs && npm publish && semantic-release post",
    "test": "standard && node test/index.js",
    "test:osa": "npm run cjs && webpack --config test/webpack.config.js && osascript test/dist/index.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "cz-conventional-changelog": "^1.2.0",
    "editorconfig": "^0.13.2",
    "minimatch": "^3.0.3",
    "rollup": "^0.36.4",
    "rollup-plugin-alias": "^1.2.0",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-json": "^2.0.2",
    "rollup-plugin-node-resolve": "^2.0.0",
    "semantic-release": "^6.3.2",
    "standard": "^8.6.0",
    "tape": "^4.6.3",
    "webpack": "^2.1.0-beta"
  }
}
