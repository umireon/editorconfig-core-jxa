{
  "name": "editorconfig-jxa",
  "version": "0.2.0",
  "description": "EditorConfig core library for JXA (JavaScript for Automation) on macOS",
  "keywords": ["jxa", "osx", "macos"],
  "homepage": "https://github.com/umireon/editorconfig-core-jxa",
  "bugs": {
    "url": "https://github.com/umireon/editorconfig-core-jxa/issues"
  },
  "license": "MIT",
  "author": "Kaito Udagawa <umireon@gmail.com>",
  "files": [
    "dist/*"
  ],
  "main": "dist/editorconfig.js",
  "module": "dist/editorconfig.mjs",
  "repository": "umireon/editorconfig-jxa",
  "scripts": {
    "build": "npm run cjs && npm run es && npm run scpt",
    "cjs": "rollup -c -f cjs -o dist/editorconfig.js",
    "es": "rollup -c -f es -o dist/editorconfig.mjs",
    "scpt": "rollup -c -f cjs --intro '!function(exports){' --outro '}(this)' | osacompile -l JavaScript -o dist/editorconfig.scpt"
  },
  "devDependencies": {
    "editorconfig": "^0.13.2",
    "eslint": "^3.11.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "minimatch": "^3.0.3",
    "rollup": "^0.36.4",
    "rollup-plugin-alias": "^1.2.0",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1"
  },
  "os": ["darwin"]
}
